{
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 2020
  },
  "env": {
    "browser": true,
    "node": true
  },
  "plugins": ["@docusaurus"],
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:@docusaurus/recommended"
  ],
  "rules": {
    "react/prop-types": 0
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
